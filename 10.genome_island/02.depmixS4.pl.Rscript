library('depmixS4')
data=read.table('01.merge.pl.txt',header=T)
set.seed(31415)
mod = depmix(list(fst~1),ns=3,family=list(gaussian()),data=data,ntimes=c(1003,496,433,474,512,548,306,385,253,445,364,310,319,372,301,285,317,329,309))
fm <- fit(mod)
info=summary(fm)
write.table(info,file="divergenceIsland.info",quote=F)
a=posterior(fm)
write.table(a,file="divergenceIsland.txt",quote=F)